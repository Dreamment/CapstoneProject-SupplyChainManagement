@{
    ViewData["Title"] = "Home Page";
}
@model HomeViewModel

@if(Model.RoleName == "Supplier")
{
    <partial name="_SupplierIndex" model="(Model.SupplierHomeGetTendersDTO, Model.UserName)" />
}
else if (Model.RoleName == "Admin")
{
    <partial name="_AdminIndex" model="(Model.AdminHomeGetTendersDTO, Model.UserName)" />
}